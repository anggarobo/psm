{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "composite": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "typeRoots": ["./node_modules/@types", "./types"],
    "types": []
  },
  "include": ["src", "src/renderer", "assets", "types.d.ts", "declare.d.ts"]
}
